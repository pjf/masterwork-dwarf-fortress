			ttl.SetItem (5,"	[CASTE_NAME:" + awesomename + ":" + Pluralize(awesomename) + ":" + awesomename + "]")
			ttl.SetItem (2,"	[NAME:" + awesomename + ":" + Pluralize(awesomename) + ":" + awesomename + "]")
